class Solution {
public:
    bool hasCycle(ListNode *head) {
         if(head==NULL||head->next==NULL) return false;
        ListNode *p=head;
        ListNode *q=head->next;
       
        while(p!=q){
            if((q==nullptr) || (q->next==nullptr)) return false;
            p=p->next;
            q=q->next->next;
        }
        return true;
    }
};
2ã€âœŒï¸å¿«æ…¢æŒ‡é’ˆæ³•ï¼ˆ2ï¼‰ //æœ€åæƒ…å†µæ—¶é—´éœ€è¦n/2
class Solution {
public:
    bool hasCycle(ListNode *head) {
        if (head == nullptr || head->next == nullptr) return false;//æ’é™¤åªæœ‰ä¸€ä¸ªå…ƒç´ æˆ–æ— å…ƒç´ æƒ…å†µ
        ListNode *fast = head;//è®¾ç½®å¿«æ…¢æŒ‡é’ˆ
        ListNode *slow = head;
        while (fast != nullptr && fast->next != nullptr) {//æ¢ä¸€ç§æ–¹å¼ï¼Œåœ¨nullptrå‰å¾ªç¯
            fast = fast->next->next;//ä¸€æ¬¡ä¸¤ä¸ª
            slow = slow->next;//ä¸€æ¬¡ä¸€ä¸ª
            if (fast == slow) return true;//è‹¥ç›¸ç­‰è¯´æ˜æ˜¯ç¯å½¢
        }
        return false;//è‹¥ä¸ç›¸ç­‰ä¸”é€€å‡ºåˆ™éç¯æ€§
    }
};
