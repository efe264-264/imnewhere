class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {
vector<int>nums3;
int a =0;
int b= 0;

while ((a <m)&&( b <n))//æƒ…å†µä¸€ï¼ŒæŒ‡é’ˆéƒ½æ²¡åˆ°æœ€å°¾æ—¶æˆ–åŒæ—¶åˆ°å°¾
{
    if (nums1[a] > nums2[b]) { nums3.push_back(nums2[b]); b++; }
    else { nums3.push_back(nums1[a]); a++; }
}
while ((a >= m) && (b < n))//æƒ…å†µäºŒï¼ŒæŒ‡é’ˆåˆ°1å°¾æ²¡åˆ°2å°¾(æ³¨æ„é˜²æ­¢æº¢å‡ºï¼‰
{
    nums3.push_back(nums2[b]); b++;
}
while ((a < m) && (b >= n))//æƒ…å†µäºŒï¼ŒæŒ‡é’ˆåˆ°1å°¾æ²¡åˆ°2å°¾(æ³¨æ„é˜²æ­¢æº¢å‡ºï¼‰
{
        nums3.push_back(nums1[a ]); a++;

}
for (int i = 0; i < m + n; i++) {
    nums1[i] = nums3[i];
}
       
    }
};
